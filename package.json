{
  "name": "ariadne-ts",
  "version": "0.1.5-alpha.10",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src/"
  ],
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn run clean:dist",
    "build": "tsc",
    "clean:dist": "rm -rf dist/ && mkdir dist",
    "clean:public": "rm -rf public/ && mkdir public",
    "prebundle": "yarn run clean:public && cp www/index.html public/index.html",
    "bundle": "webpack --mode=production",
    "bundle:analyze": "cross-env ENABLE_ANALYZER=true yarn bundle",
    "bundle:dev": "webpack serve --open",
    "compile": "npm run bundle && npm run build",
    "example": "sh scripts/run-example.sh",
    "example:watch": "sh scripts/watch-example.sh",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/node": "^18.0.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/string-format": "^2.0.0",
    "assert": "^2.0.0",
    "babel-cli": "6",
    "babel-preset-react-app": "3",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "formik": "^2.2.9",
    "nodemon": "^2.0.19",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "react-dom": "^18.2.0",
    "react-tabs": "^5.1.0",
    "string-format": "^2.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
