    "typescript": "^4.7.4",
    "util": "^0.12.4",
    "vitest": "^0.18.0",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "xterm": "^4.19.0",
    "xterm-addon-fit": "^0.5.0",
    "xterm-addon-search": "^0.9.0",
    "xterm-addon-unicode11": "^0.3.0",
    "xterm-for-react": "^1.0.4",
    "yup": "^0.32.11",
    "zustand": "^4.0.0-rc.4"
  }
